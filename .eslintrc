{
  "extends": [
    "react-app",
    "standard",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "no-console": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "space-before-function-paren": 0,
    "no-unused-vars": 2,
    "prettier/prettier": "error"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  }
}
